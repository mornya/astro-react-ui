  "license": "MIT",
  "author": "mornya <mornya78@gmail.com>",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mornya/astro-react-ui.git"
  },
  "type": "module",
  "types": "./types",
  "engines": {
    "node": ">=18.18 || >=20.11",
